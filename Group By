/**
 * @param {Function} fn
 * @return {Object}
 */
Array.prototype.groupBy = function(fn) {
    const array = this
    const result = {};
    for (let i = 0; i < array.length; i++) {
        const key = fn(array[i])

        if (!result[key]) {
            result[key] = []
        }

        result[key].push(array[i])

    }
    return result;

};

/**
 * [1,2,3].groupBy(String) // {"1":[1],"2":[2],"3":[3]}
 */
