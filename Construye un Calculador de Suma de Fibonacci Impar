function sumFibs(num) {
  let fibSum = 0
  const listFib = [0, 1]
  while (true) {
    let sum = listFib[listFib.length - 2] + listFib[listFib.length - 1] 
    if (sum <= num) {
      listFib.push(sum)
    } else {
      break
    }
    
  }
  
  for (let i = 0; i < listFib.length ; i++) {
    if (listFib[i] % 2 !== 0) {
      fibSum += listFib[i]
      
    }
  }
  
  return fibSum
}

console.log(sumFibs(75025))
