/**
 * @param {Array} arr1
 * @param {Array} arr2
 * @return {Array}
 */
var join = function(arr1, arr2) {
    const registro = {}
    for (let i = 0; i < arr1.length; i++){
        registro[arr1[i].id] = arr1[i]
    }
    
    for (let o = 0; o < arr2.length; o++){
        if (arr2[o].id in registro) {
            registro[arr2[o].id] = {...registro[arr2[o].id], ...arr2[o] };
        } else {
            registro[arr2[o].id] = arr2[o]
        }
    }
    return Object.values(registro)
};
